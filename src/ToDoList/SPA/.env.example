# React SPA Configuration
# Copy this file to .env.local and fill with your actual values
# React environment variables must start with REACT_APP_

# Azure AD / Microsoft Entra External ID Configuration
REACT_APP_TENANT_ID=00000000-0000-0000-0000-000000000000
REACT_APP_TENANT_SUBDOMAIN=your-tenant-subdomain
REACT_APP_CLIENT_ID=00000000-0000-0000-0000-000000000000

# ToDoList API Configuration
REACT_APP_API_CLIENT_ID=00000000-0000-0000-0000-000000000000
REACT_APP_API_ENDPOINT=http://localhost:5000/api/todolist

# Optional: Uncomment and configure for production
# REACT_APP_REDIRECT_URI=https://yourdomain.com
# REACT_APP_POST_LOGOUT_REDIRECT_URI=https://yourdomain.com
